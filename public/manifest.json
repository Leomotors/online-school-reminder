{
  "manifest_version": 3,
  "name": "Online School Reminder",
  "description": "Chrome Extension that will remind you to enter class",
  "version": "1.0.420",
  "action": {
    "default_icon": "Jeff.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js"]
    }
  ],
  "permissions": ["notifications", "tabs"],
  "host_permissions": ["<all_urls>"]
}
