{
  "manifest_version": 3,
  "name": "Online School Reminder",
  "description": "Chrome Extension that will remind you to enter class",
  "version": "1.0.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["notifications"],

  "host_permissions": ["<all_urls>"]
}
